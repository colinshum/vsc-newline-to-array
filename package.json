{
  "name": "convert-newline-list-to-array",
  "displayName": "Convert Newline List to Array",
  "description": "Converts lists split by newline to comma-separated array",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:convert-newline-list-to-array.convertNewLineToArray"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "convert-newline-list-to-array.convertNewLineToArray",
        "title": "Convert New Line List to Array"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
